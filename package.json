    "@types/mongodb": "^3.1.19",
    "benchmark": "^2.1.4",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "istanbul": "v1.1.0-alpha.1",
    "jest": "^23.6.0",
    "mocha": "^5.1.1",
